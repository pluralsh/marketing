{
  "format": "custom",
  "id": "settings",
  "label": "Settings",
  "repeatable": false,
  "status": true,
  "json": {
    "Default SEO": {
      "meta_title_template": {
        "type": "Text",
        "config": {
          "label": "Meta Title Template",
          "placeholder": "eg. %s | My Website"
        }
      },
      "meta_title": {
        "type": "Text",
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        }
      },
      "meta_description": {
        "type": "Text",
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        }
      },
      "meta_keywords": {
        "type": "Text",
        "config": {
          "label": "Meta Keywords",
          "placeholder": ""
        }
      },
      "meta_image": {
        "type": "Image",
        "config": {
          "label": "Meta Image",
          "constraint": {
            "width": 2400,
            "height": 1260
          },
          "thumbnails": []
        }
      }
    },
    "Navigation": {
      "nav_logo": {
        "type": "Image",
        "config": {
          "label": "Logo",
          "constraint": {},
          "thumbnails": []
        }
      },
      "nav_logo_redirect": {
        "type": "Link",
        "config": {
          "label": "Logo Redirect",
          "placeholder": "Targets homepage by default",
          "select": null,
          "allowTargetBlank": true,
          "allowText": false,
          "repeat": false
        }
      },
      "nav_buttons": {
        "type": "Link",
        "config": {
          "label": "Buttons",
          "placeholder": "",
          "select": null,
          "allowTargetBlank": true,
          "allowText": true,
          "repeat": true,
          "variants": ["Primary", "Secondary"]
        }
      },
      "slices2": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "navigation_item": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "Footer": {
      "footer_logo": {
        "type": "Image",
        "config": {
          "label": "Logo",
          "constraint": {},
          "thumbnails": []
        }
      },
      "footer_logo_redirect": {
        "type": "Link",
        "config": {
          "label": "Logo Redirect",
          "placeholder": "Targets homepage by default",
          "select": null,
          "allowTargetBlank": true,
          "allowText": false,
          "repeat": false
        }
      },
      "footer_links": {
        "type": "Group",
        "config": {
          "label": "Links",
          "repeat": true,
          "fields": {
            "group_name": {
              "type": "Link",
              "config": {
                "label": "Group Name",
                "placeholder": "",
                "select": null,
                "allowTargetBlank": true,
                "allowText": true,
                "repeat": false
              }
            },
            "child_links": {
              "type": "Link",
              "config": {
                "label": "Child Links",
                "placeholder": "",
                "select": null,
                "allowTargetBlank": true,
                "allowText": true,
                "repeat": true
              }
            }
          }
        }
      },
      "footer_legal_links": {
        "type": "Link",
        "config": {
          "label": "Legal Links",
          "placeholder": "",
          "select": null,
          "allowTargetBlank": true,
          "allowText": true,
          "repeat": true
        }
      }
    },
    "Social Media": {
      "social_bluesky": {
        "type": "Link",
        "config": {
          "label": "Bluesky",
          "placeholder": "",
          "select": null,
          "allowTargetBlank": false,
          "allowText": false,
          "repeat": false
        }
      },
      "social_facebook": {
        "type": "Link",
        "config": {
          "label": "Facebook",
          "placeholder": "",
          "select": null,
          "allowTargetBlank": false,
          "allowText": false,
          "repeat": false
        }
      },
      "social_linkedin": {
        "type": "Link",
        "config": {
          "label": "Linkedin",
          "placeholder": "",
          "select": null,
          "allowTargetBlank": false,
          "allowText": false,
          "repeat": false
        }
      },
      "social_pinterest": {
        "type": "Link",
        "config": {
          "label": "Pinterest",
          "placeholder": "",
          "select": null,
          "allowTargetBlank": false,
          "allowText": false,
          "repeat": false
        }
      },
      "social_tiktok": {
        "type": "Link",
        "config": {
          "label": "Tiktok",
          "placeholder": "",
          "select": null,
          "allowTargetBlank": false,
          "allowText": false,
          "repeat": false
        }
      },
      "social_twitter": {
        "type": "Link",
        "config": {
          "label": "X/Twitter",
          "placeholder": "",
          "select": null,
          "allowTargetBlank": false,
          "allowText": false,
          "repeat": false
        }
      },
      "social_youtube": {
        "type": "Link",
        "config": {
          "label": "YouTube",
          "placeholder": "",
          "select": null,
          "allowTargetBlank": false,
          "allowText": false,
          "repeat": false
        }
      }
    }
  }
}
