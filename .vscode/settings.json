{
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": [
    "source.formatDocument",
    "source.fixAll.eslint"
  ],
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "dotenv.enableAutocloaking": false,
  "files.associations": {
    "*.md": "markdoc",
    "*.mdpart": "markdoc"
  }
}