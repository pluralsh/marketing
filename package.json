    "postcss": "8.4.27",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.11",
    "react-hooks": "1.0.1",
    "stylelint": "15.10.1",
    "stylelint-config-prettier": "9.0.5",
    "tailwind-merge": "2.2.1",
    "tailwindcss": "3.3.3",
    "typescript": "5.3.3"
  },
  "packageManager": "yarn@3.7.0",
  "lint-staged": {
    "./{src,pages}/**/*.{js,jsx,ts,tsx,graphql,md,mdpart}": "prettier --write"
  }
