    "react-spring": "9.7.2",
    "react-transition-group": "4.4.5",
    "react-use-measure": "2.1.1",
    "rehype-raw": "7.0.0",
    "rooks": "7.14.1",
    "styled-components": "5.3.11",
    "swiper": "9.4.1",
    "swr": "2.2.0",
    "tslib": "2.6.0",
    "type-fest": "4.8.3",
